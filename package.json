{
  "name": "cyberforce-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "styles:build": "npx gulp buildStyles",
    "styles:watch": "npm run styles:build && ./node_modules/.bin/gulp watchStyles",
    "server:watch": "./node_modules/.bin/nodemon -e pug,js -w ./src -x node ./bin/www",
    "server": "node ./bin/www",
    "local": "./node_modules/.bin/npm-run-all -p styles:watch server:watch",
    "start": "./node_modules/.bin/npm-run-all styles:build server"
  },
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.1",
    "cookie-parser": "~1.4.4",
    "cssnano": "^4.1.10",
    "debug": "~2.6.9",
    "eslint": "^6.8.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "~4.16.1",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglifycss": "^1.1.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4"
  }
}
